{
	"manifest_version": 2,
	"name": "Kitco",
	"version": "1.0",
	"description": "Kitco for Chrome",
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "kitco.html"
	},
	"icons": {
	    "16": "images/icon.png",
	    "48": "images/48.png",
	    "128": "images/128.png"
	},
	"permissions": [
		"http://www.kitco.com/*"
	],
	"background": {
		"page": "background.html"
	}
}
